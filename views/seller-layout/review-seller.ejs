    <!-- container -->
    <div class="container">
        <div class="row mt-5">
            <div class="col-md-4">
                <div class="profile-sidebar">
                    <!-- card profile -->
                    <div class="card">
                        <img src="../img/profile/user<%= user.ID %>.png" alt="John" style="width:100%">
                        <h1 class="mt-3">
                            <%= user.name %>
                        </h1>
                        <p class="title">CEO & Founder, Example</p>
                        <p><a href="#"><i class="fa fa-dribbble"></i></a>
                            <a href="#"><i class="fa fa-twitter"></i></a>
                            <a href="#"><i class="fa fa-linkedin"></i></a>
                            <a href="#"><i class="fa fa-facebook"></i></a></p>
                        <div>
                            <button class="btn-contact">Contact</button>
                        </div>
                        <div>
                            <hr class="featurette-divider ">
                            <span class="heading"><b>Your Rating</b></span>
                            <div class="row-rate">
                                <div class="side">
                                    <div><a><i class="far fa-thumbs-up"></i></a></div>
                                </div>
                                <div class="middle">
                                    <div class="bar-container">
                                        <div class="bar-up"></div>
                                    </div>
                                </div>
                                <div class="side right">
                                    <div>
                                        <%= user.numLike %>
                                    </div>
                                </div>
                            </div>
                            <div class="row-rate">
                                <div class="side">
                                    <div><a><i class="far fa-thumbs-down"></i></a></div>
                                </div>
                                <div class="middle">
                                    <div class="bar-container">
                                        <div class="bar-down"></div>
                                    </div>
                                </div>
                                <div class="side right">
                                    <div>
                                        <%= user.numDislike %>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div>
                            <hr class="featurette-divider ">
                            <p><b>You are a seller </b></p>
                        </div>
                        <div>
                            <hr class="featurette-divider ">
                            <p><b>Member Since:</b></p>
                            <p class="text-muted">
                                <%= user.dateJoin %>
                            </p>
                        </div>
                        <div>
                            <hr class="featurette-divider ">
                            <p><b>Total Buy:</b></p>
                            <p class="text-muted">250</p>
                        </div>
                        <div>
