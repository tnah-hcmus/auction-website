<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Hello Auction</title>

    <!-- Bootstrap core CSS -->
    <link href="../vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom fonts for this template -->
    <link href="../vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
    <link href='https://fonts.googleapis.com/css?family=Fira+Sans|Kaushan+Script' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <!--  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css"> -->

    <!-- Custom styles for this template -->
    <link href="../css/seller-auctioned-product.css" rel="stylesheet">

</head>

<body id="page-top">

    <!-- Navigation -->

    <% include ../partial-layout/header%>

        <!-- Header -->
        <header class="masthead">
            <div class="container">
                <div class="input-group search-bar text-center">
                    <div class="input-group-prepend dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown">
                            <i class="fas fa-filter"></i>
                        </a>
                        <div class="dropdown-menu">
                            <a class="dropdown-item" href="#">Search by name</a>
                            <a class="dropdown-item" href="#">Search by category</a>
                        </div>
                    </div>
                    <input type="text" class="form-control" placeholder="Search your goods">
                    <div class="input-group-append">
                        <button class="btn btn-secondary" type="button"><i class="fa fa-search"></i></button>
                    </div>

                </div>
                <br> <br>
            </div>
        </header>

        <!-- container -->
        <div class="container">
            <div class="row mt-5">
                <div class="col-md-4">
                    <div class="profile-sidebar">
                        <!-- card profile -->
                        <div class="card">
                            <img src="../img/profile/user<%= user.ID %>.png" alt="John" style="width:100%">
                            <h1 class="mt-3">
                                <%= user.name %>
                            </h1>
                            <p class="title">CEO & Founder, Example</p>
                            <p><a href="#"><i class="fa fa-dribbble"></i></a>
                                <a href="#"><i class="fa fa-twitter"></i></a>
                                <a href="#"><i class="fa fa-linkedin"></i></a>
                                <a href="#"><i class="fa fa-facebook"></i></a></p>
                            <div>
                                <button class="btn-contact">Contact</button>
                            </div>
                            <div>
                                <hr class="featurette-divider ">
                                <span class="heading"><b>Your Rating</b></span>
                                <div class="row-rate">
                                    <div class="side">
                                        <div><a><i class="far fa-thumbs-up"></i></a></div>
                                    </div>
                                    <div class="middle">
                                        <div class="bar-container">
                                            <div class="bar-up"></div>
                                        </div>
                                    </div>
                                    <div class="side right">
                                        <div>
                                            <%= user.numLike %>
                                        </div>
                                    </div>
                                </div>
                                <div class="row-rate">
                                    <div class="side">
                                        <div><a><i class="far fa-thumbs-down"></i></a></div>
                                    </div>
                                    <div class="middle">
                                        <div class="bar-container">
                                            <div class="bar-down"></div>
                                        </div>
                                    </div>
                                    <div class="side right">
                                        <div>
                                            <%= user.numDislike %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div>
                                <hr class="featurette-divider ">
                                <p><b>You are a seller </b></p>
                            </div>
                            <div>
                                <hr class="featurette-divider ">
                                <p><b>Member Since:</b></p>
                                <p class="text-muted">
                                    <%= user.dateJoin %>
                                </p>
                            </div>
                            <div>
                                <hr class="featurette-divider ">
                                <p><b>Total Buy:</b></p>
                                <p class="text-muted">250</p>
                            </div>
                            <div>
                                <hr class="featurette-divider ">
                                <p><b>Web site:</b></p>
                                <p class="text-muted">www.1234.com</p>
                            </div>
                            <div>
                                <hr class="featurette-divider ">
                                <p><button>Account setting</button></p>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-lg-8">
                    <!-- menu -->
                    <div class="topnav mt-5">
                        <a href="seller-watchlist.html">Watch list</a>
                        <a href="seller-bidding.html">Bidding products</a>
                        <a href="seller-wonproduct.html">Won products</a>
                        <a href="seller-review.html">Reviews</a>
                        <!-- <a href="#contact">Account setting</a> -->
                    </div>
                    <div class="topnav mt-7">
                        <a href="seller-auctioned-product.html" class="active">Auctioned product</a>
                        <a href="seller-myproduct.html">My product</a>
                    </div>
                    <div class="profile-content">
                        <!-- tab menu content -->
                        <!-- Main View -->
                        <section class="bg-light page-section" id="list">
                            <div class="container">

                                <div class="row">
                                    <% auctionedPro.forEach(function(item) { %>
                                        <div class="col-md-4 col-sm-6 list-item">
                                            <img class="img-fluid" src="../img/list/<%= item.imgID %>-thumbnail.jpg" alt="">
                                            <div class="list-caption">
                                                <h4>
                                                    <%= item.name %>
                                                </h4>
                                                <p class="text-muted">Bid price: $
                                                    <%= item.price %>
                                                </p>
                                                <hr class="featurette-divider ">
                                                <p class="mb-5"><span class="dot"><img class="avatar"src="../img/profile/user<%= user.ID %>.png"> <b><%= item.ownerName %></b></span>
                                                    <a class="mr-2" data-toggle="modal" data-target="#review-up-<%= item.ID %>"><i class="far fa-thumbs-up"></i></a>
                                                    <a class="mr-3" data-toggle="modal" data-target="#review-down-<%= item.ID %>"><i class="far fa-thumbs-down"></i></a></p>
                                                <button id="cancel-transaction" type="button" class="btn-cancel-transaction btn-outline-warning" data-toggle="modal" data-target="#cancel-<%= item.ID %>"> Cancel transaction</button>
                                            </div>
                                        </div>
                                        <% }) %>
                                </div>

                                <ul class="pagination justify-content-center">
                                    <li class="page-item disabled"><a class="page-link" href="#">Previous</a></li>
                                    <% pagination.forEach(function(page) { %>
                                        <li class="page-item">
                                            <a class="page-link" href="#">
                                                <%= page.number %>
                                            </a>
                                        </li>
                                        <% }) %>
                                            <li class="page-item"><a class="page-link" href="#">Next</a></li>
                                </ul>
                            </div>
                        </section>


                    </div>

                </div>
            </div>

            <!-- list Modals -->

            <!-- review up modal -->
            <% reviewPro.forEach(function(item) { %>
                <div class="modal fade" id="review-up-<%= item.ID %>" role="dialog">
                    <div class="modal-dialog">

                        <!-- Modal content-->
                        <div class="modal-content">
                            <div class="modal-header">
                                <h4 class="modal-title">Your review</h4>
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                            </div>
                            <div class="modal-body">
                                <textarea id="subject" name="subject" placeholder="Write your review.."></textarea>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-default" data-dismiss="modal">Send</button>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- review down modal -->
                <div class="modal fade" id="review-down-<%= item.ID %>" role="dialog">
                    <div class="modal-dialog">

                        <!-- Modal content-->
                        <div class="modal-content">
                            <div class="modal-header">
                                <h4 class="modal-title">Your review</h4>
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                            </div>
                            <div class="modal-body">
                                <textarea id="subject" name="subject" placeholder="Write your review.."></textarea>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-default" data-dismiss="modal">Send</button>
                            </div>
                        </div>

                    </div>
                </div>

                <!-- review cancel modal -->
                <div class="modal fade" id="cancel-<%= item.ID %>" role="dialog">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h4 class="modal-title">Your review</h4>
                                <button type="button" class="close" data-dismiss="modal">&times;</button>
                            </div>
                            <div class="modal-body">
                                Do you really want to cancel the transaction ?
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-default" data-dismiss="modal">Yes</button>
                                <button type="button" class="btn btn-default" data-dismiss="modal">No</button>
                            </div>
                        </div>
                    </div>
                </div>
                <% }) %>


                    <!-- Footer -->
                    <% include ../partial-layout/footer%>



                        <!-- Bootstrap core JavaScript -->
                        <script src="../vendor/jquery/jquery.min.js"></script>
                        <script src="../vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

                        <!-- Plugin JavaScript -->
                        <script src="../vendor/jquery-easing/jquery.easing.min.js"></script>


                        <!-- Custom scripts for this template -->
                        <script src="../js/bidder-watchlist.js"></script>



</body>

</html>