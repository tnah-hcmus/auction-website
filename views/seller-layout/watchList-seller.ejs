<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Hello Auction</title>

    <!-- Bootstrap core CSS -->
    <link href="../vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom fonts for this template -->
    <link href="../vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
    <link href='https://fonts.googleapis.com/css?family=Fira+Sans|Kaushan+Script' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <!-- Custom styles for this template -->
    <link href="../css/seller-watchlist.css" rel="stylesheet">

</head>

<body id="page-top">
    <!-- Navigation -->
    <% include ../partial-layout/header%>

        <!-- Header -->
        <header class="masthead">
            <div class="container">
                <div class="input-group search-bar text-center">
                    <div class="input-group-prepend dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown">
                            <i class="fas fa-filter"></i>
                        </a>
                        <div class="dropdown-menu">
                            <a class="dropdown-item" href="#">Search by name</a>
                            <a class="dropdown-item" href="#">Search by category</a>
                        </div>
                    </div>
                    <input type="text" class="form-control" placeholder="Search your goods">
                    <div class="input-group-append">
                        <button class="btn btn-secondary" type="button"><i class="fa fa-search"></i></button>
                    </div>

                </div>
                <br> <br>
            </div>
        </header>

        <!-- container -->
        <div class="container">
            <div class="row mt-5">
                <div class="col-md-4">
                    <div class="profile-sidebar">
                        <!-- card profile -->
                        <div class="card">
                            <img src="../img/profile/user<%= user.ID %>.png" alt="John" style="width:100%">
                            <h1 class="mt-3">
                                <%= user.name %>
                            </h1>
                            <p class="title">CEO & Founder, Example</p>
                            <p><a href="#"><i class="fa fa-dribbble"></i></a>
                                <a href="#"><i class="fa fa-twitter"></i></a>
                                <a href="#"><i class="fa fa-linkedin"></i></a>
                                <a href="#"><i class="fa fa-facebook"></i></a></p>
                            <div>
                                <button class="btn-contact">Contact</button>
                            </div>
                            <div>
                                <hr class="featurette-divider ">
                                <span class="heading"><b>Your Rating</b></span>
                                <div class="row-rate">
                                    <div class="side">
                                        <div><a><i class="far fa-thumbs-up"></i></a></div>
                                    </div>
                                    <div class="middle">
                                        <div class="bar-container">
                                            <div class="bar-up"></div>
                                        </div>
                                    </div>
                                    <div class="side right">
                                        <div>
                                            <%= user.numLike %>
                                        </div>
                                    </div>
                                </div>
                                <div class="row-rate">
                                    <div class="side">
                                        <div><a><i class="far fa-thumbs-down"></i></a></div>
                                    </div>
                                    <div class="middle">
                                        <div class="bar-container">
                                            <div class="bar-down"></div>
                                        </div>
                                    </div>
                                    <div class="side right">
                                        <div>
                                            <%= user.numDislike %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div>
                                <hr class="featurette-divider ">
                                <p><b>You are a seller </b></p>
                            </div>
                            <div>
                                <hr class="featurette-divider ">
                                <p><b>Member Since:</b></p>
                                <p class="text-muted">
                                    <%= user.dateJoin %>
                                </p>
                            </div>
                            <div>
                                <hr class="featurette-divider ">
                                <p><b>Total Buy:</b></p>
                                <p class="text-muted">250</p>
                            </div>
                            <div>
                                <hr class="featurette-divider ">
                                <p><b>Web site:</b></p>
                                <p class="text-muted">www.1234.com</p>
                            </div>
                            <div>
                                <hr class="featurette-divider ">
                                <p><button>Account setting</button></p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-8">
                    <!-- menu -->
                    <div class="topnav mt-5">
                        <a href="seller-watchlist.html" class="active">Watch list</a>
                        <a href="seller-bidding.html">Bidding products</a>
                        <a href="seller-wonproduct.html">Won products</a>
                        <a href="seller-review.html">Reviews</a>
                        <!-- <a href="#contact">Account setting</a> -->
                    </div>
                    <div class="topnav mt-7">
                        <a href="seller-auctioned-product.html">Auctioned product</a>
                        <a href="seller-myproduct.html">My product</a>
                    </div>
                    <div class="profile-content">
                        <!-- tab menu content -->
                        <!-- Main View -->
                        <section class="bg-light page-section" id="list">
                            <div class="container">
                                <div class="row">
                                    <% watchPro.forEach(function(item) { %>

                                        <div class="col-md-4 col-sm-6 list-item">
                                            <a class="list-link" data-toggle="modal" href="#chart-item-<%= item.ID %>">
                                                <div class="list-hover">
                                                    <div class="list-hover-content">
                                                        <i class="fas fa-plus fa-3x"></i>
                                                    </div>
                                                </div>
                                                <img class="img-fluid" src="../img/list/<%= item.imgID %>-thumbnail.jpg" alt="">
                                            </a>
                                            <div class="list-caption">
                                                <h4>
                                                    <%= item.name %>
                                                </h4>
                                                <p class="text-muted">Owner:
                                                    <%= item.ownerName %>
                                                </p>
                                                <p class="text-muted">Date released:
                                                    <%= item.dateRelease %>
                                                </p>
                                            </div>
                                        </div>
                                        <% }) %>
                                </div>
                                <ul class="pagination justify-content-center">
                                    <li class="page-item disabled"><a class="page-link" href="#">Previous</a></li>
                                    <% pagination.forEach(function(page) { %>
                                        <li class="page-item">
                                            <a class="page-link" href="#">
                                                <%= page.number %>
                                            </a>
                                        </li>
                                        <% }) %>
                                            <li class="page-item disabled"><a class="page-link" href="#">Next</a></li>
                                </ul>
                            </div>
                        </section>
                        <!-- list Modals -->

                        <!-- Modal  -->
                        <% modalPro.forEach(function(item) { %>
                            <div class="list-modal modal fade" id="chart-item-<%= item.ID %>" tabindex="-1" role="dialog" aria-hidden="true">
                                <div class="modal-dialog">
                                    <div class="modal-content">
                                        <div class="close-modal" data-dismiss="modal">
                                            <div class="lr">
                                                <div class="rl"></div>
                                            </div>
                                        </div>
                                        <div class="container">
                                            <div class="row featurette ">
                                                <div class="col-lg-5 mx-auto">
                                                    <div class="modal-body">
                                                        <img class="img-fluid d-block mx-auto mt-5" src="../img/top-chart/<%= item.imgID %>-full.jpg" alt="">
                                                        <div>
                                                            <button data-toggle="tooltip" data-placement="bottom" title="View details product" type="button" class="btn-bid  btn-outline-warning"><a href="../main-views/detail-product.html"><i class="fas fa-search"></i></a></button>
                                                            <button data-toggle="tooltip" data-placement="bottom" title="Add to watch list " type="button" class="btn-bid btn-outline-warning"><i class="far fa-heart"></i></button>
                                                            <button data-toggle="tooltip" data-placement="bottom" title="Bid now" type="button" class="btn-bid btn-outline-warning"><i class="fas fa-gavel"></i>
                                                  </button>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-lg-7 mx-auto">
                                                    <h1 class=" product-title ">
                                                        <%= item.name %>
                                                    </h1>
                                                    <h1 class=" product-price mt-4"> Current Bid: $
                                                        <%= item.currentBid %>
                                                    </h1>
                                                    <hr class="featurette-divider">
                                                    <!-- Project Details Go Here -->
                                                    <p class="content">ABCDE laptop on the lid is designed with aluminum alloy with characteristic concentric circles, in addition to the surface is covered with a layer of flashy diamond imitation with advanced nano embossing
                                                        technology, "light weight" standard with 1.2kg weight. The design of the device tapers towards the keyboard, so holding the phone or moving is still comfortable and easy.</p>
                                                    <button type="button" class="btn-buynow btn-warning  mb-5">BUY NOW FOR $<%= item.buyNow %></button>
                                                    <ul class="list-inline content mx-auto">
                                                        <li>People post the product:
                                                            <%= item.owner %>
                                                        </li>
                                                        <li>Posted date:
                                                            <%= item.datePost %>
                                                        </li>
                                                        <li>End date:
                                                            <%= item.dateEnd %>
                                                        </li>
                                                        <li>Category:
                                                            <%= item.cat %>
                                                        </li>
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <% }) %>

                    </div>
                </div>

                <!-- Footer -->
                <% include ../partial-layout/footer%>



                    <!-- Bootstrap core JavaScript -->
                    <script src="../vendor/jquery/jquery.min.js"></script>
                    <script src="../vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

                    <!-- Plugin JavaScript -->
                    <script src="../vendor/jquery-easing/jquery.easing.min.js"></script>

                    <!-- Custom scripts for this template -->
                    <script src="../js/bidder-bidding.js"></script>




</body>

</html>